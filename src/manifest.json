{

  "description": "Turn your new tab page into win95 inspired desktop, where all your bookmarks are easy to reach",
  "manifest_version": 3,
  "name": "CWNewTab",
  "version": "1.0.1",
  "browser_specific_settings": {
    "gecko": {
      "id": "cw_newtab@example.com"
    }
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "chrome_settings_overrides": {
    "homepage": "index.html"
  },
  "content_security_policy": {
    "default-src": "none",
    "img-src": "https:",
    "script-src": "self"
  },
  "permissions": [
    "bookmarks",
    "storage",
    "unlimitedStorage"
  ]

}
